<feature_model name="Telecommunication_System">
<feature_tree>
:r tecom
	:o ipvoice (ipvoice)
		:m software
			:g [1,1]
				: swpack1 (swpack1)
				: swpack2
	:m rack
		:m sizes
			:g [1,1]
				: large
				: medium
				: small
	:o messaging (messaging)
		:o upgrade_swpack (upgrade_swpack)
</feature_tree>
<constraints>
c1: (~swpack1) or (~upgrade_swpack)
c2: (~ipvoice) or (~messaging) or upgrade_swpack
</constraints>
</feature_model>

